#include <stdio.h>

int main() {
    int numero;

    printf("Digite um numero: ");
    scanf("%d", &numero);

    if (numero % 5 == 0) {
        printf("%d e divisivel por 5\n", numero);
    }
    if (numero % 3 == 0) {
        printf("%d e divisivel por 3\n", numero);
    }
    if (numero % 2 == 0) {
        printf("%d e divisivel por 2\n", numero);
    }
    if (numero % 5 != 0 && numero % 3 != 0 && numero % 2 != 0) {
        printf("%d nao e divisivel por 2, 3 ou 5\n", numero);
    }

    return 0;
}
