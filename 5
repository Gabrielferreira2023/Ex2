#include <stdio.h>
#include <math.h>

int main() {
    float valor;
    int notas[6] = {100, 50, 20, 10, 5, 2};
    int moedas[6] = {100, 50, 25, 10, 5, 1};

    scanf("%f", &valor);

    printf("NOTAS:\n");
    for (int i = 0; i < 6; i++) {
        int qtdNotas = floor(valor / notas[i]);
        printf("%d nota(s) de R$ %d.00\n", qtdNotas, notas[i]);
        valor -= qtdNotas * notas[i];
    }

    printf("MOEDAS:\n");
    for (int i = 0; i < 6; i++) {
        int qtdMoedas = round(valor * 100) / moedas[i];
        printf("%d moeda(s) de R$ %.2f\n", qtdMoedas, (float)moedas[i]/100);
        valor -= qtdMoedas * moedas[i] / 100.0;
    }

    return 0;
}
