#include <stdio.h>
#include <math.h>

int main() {
    float numero, quadrado, raiz;

    printf("Digite um numero: ");
    scanf("%f", &numero);

    if (numero > 0) {
        quadrado = pow(numero, 2);
        raiz = sqrt(numero);

        printf("O numero ao quadrado eh: %f\n", quadrado);
        printf("A raiz quadrada do numero eh: %f\n", raiz);
    } else {
        printf("O numero digitado nao eh positivo.\n");
    }

    return 0;
}
